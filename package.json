{
  "name": "inigochoa.me",
  "version": "3.2.1",
  "description": "The personal site of Iñigo Ochoa.",
  "author": {
    "name": "Iñigo Ochoa",
    "email": "inigochoa@gmail.com",
    "url": "https://inigochoa.me/"
  },
  "repository": "https://github.com/inigochoa/inigochoa.me",
  "license": "MIT",
  "devDependencies": {
    "@11ty/eleventy": "^0.12.1",
    "@11ty/eleventy-navigation": "^0.1.6",
    "@quasibit/eleventy-plugin-sitemap": "^2.0.4",
    "@tailwindcss/jit": "^0.1.4",
    "@tailwindcss/typography": "^0.4.0",
    "autoprefixer": "^10.2.5",
    "cssnano": "^4.1.10",
    "cypress": "^6.8.0",
    "eleventy-plugin-metagen": "^1.1.0",
    "eslint": "^7.22.0",
    "github-colors": "^2.2.19",
    "html-minifier": "^4.0.0",
    "imagemin": "^7.0.1",
    "imagemin-mozjpeg": "^9.0.0",
    "imagemin-pngquant": "^9.0.2",
    "imagemin-svgo": "^9.0.0",
    "markdown-it": "^12.0.4",
    "markdown-it-container": "^3.0.0",
    "markdown-it-for-inline": "^0.1.1",
    "markdownlint-cli2": "^0.0.14",
    "npm-run-all": "^4.1.5",
    "postcss": "^8.2.10",
    "postcss-cli": "^8.3.1",
    "postcss-nested": "^5.0.5",
    "rimraf": "^3.0.2",
    "rollup": "^2.41.1",
    "rollup-plugin-terser": "^7.0.2",
    "standard-version": "^9.2.0",
    "tailwindcss": "^2.0.3",
    "tailwindcss-debug-screens": "^2.0.0"
  },
  "scripts": {
    "clean": "rimraf _site",
    "cy:open": "cypress open",
    "cy:run": "cypress run",
    "dev": "npm-run-all clean --parallel dev:* --print-label",
    "dev:11ty": "NODE_ENV=development eleventy --serve --quiet",
    "dev:twnd": "postcss src/_assets/css/tailwind.pcss -o _site/assets/css/style.css --watch",
    "git:authors": "echo '# Authors\n' > AUTHORS.md && git log --raw | grep '^Author: ' | sort | uniq | cut -d ' ' -f2- | sed 's/^/- /' >> AUTHORS.md",
    "lint": "npm-run-all --parallel lint:* --print-label",
    "lint:js": "eslint lib src",
    "lint:md": "markdownlint-cli2 '**/*.md' '#node_modules'",
    "prod": "npm-run-all clean prod:* --print-label",
    "prod:11ty": "NODE_ENV=production eleventy --quiet",
    "prod:twnd": "postcss src/_assets/css/tailwind.pcss -o _site/assets/css/style.css --env production",
    "rel": "standard-version",
    "rel:minor": "standard-version --release-as minor",
    "rel:patch": "standard-version --release-as patch",
    "rel:major": "standard-version --release-as major"
  }
}
